package variaveis;

public class Primitivos {

	public static void main(String[] args) {
		/* Tipos primitivos em ordem hierarquica:
		 * boolean = tipo logico (true ou false)
		 * char = armazena UM caractere
		 * byte = MENOR numero inteiro (entre os inteiros - 126)
		 * short = numeros inteiros (-32 mil a 32 mil)
		 * int = numeros inteiros (padrao)
		 * long = maior entre os inteiros
		 * float - menor precisão nas casas decimais
		 * double = maior precisao nas casas decimais (ádrao) 
		 * 
		 */
		
		boolean gravida=true;
		char sexo='M';
		byte gols=0;
short quantidadecestas=0:
long cpf=0:
				float nota=0;
		double valorLitroCombustivel=0;
		
		float desconto = 500 * (float) 0.9;
		

	}

}
