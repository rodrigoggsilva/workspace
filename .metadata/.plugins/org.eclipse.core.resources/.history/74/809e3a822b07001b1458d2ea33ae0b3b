public class TesteExcecao {

	public static void main(String[] args) {
		try {
		int numero = Integer.parseInt("A");
		System.out.println(numero);
		String Rodrigo=null;
		System.out.println(Rodrigo.length());
		}catch(Exception e) {
			e.printStackTrace();
			System.out.println("Deu ruim");
		}
	}
}
